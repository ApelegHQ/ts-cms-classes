{
	"name": "@exact-realty/cms-classes",
	"version": "1.0.0",
	"description": "A collection of utility classes to encode ASN.1 data following DER rules",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "./dist/index.mjs",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./cms": {
			"import": {
				"types": "./dist/cms.d.ts",
				"default": "./dist/cms.mjs"
			},
			"require": {
				"types": "./dist/cms.d.cts",
				"default": "./dist/cms.cjs"
			}
		},
		"./pkcs10": {
			"import": {
				"types": "./dist/pkcs10.d.ts",
				"default": "./dist/pkcs10.mjs"
			},
			"require": {
				"types": "./dist/cms.d.cts",
				"default": "./dist/cms.cjs"
			}
		},
		"./x501": {
			"import": {
				"types": "./dist/x501.d.ts",
				"default": "./dist/x501.mjs"
			},
			"require": {
				"types": "./dist/x501.d.cts",
				"default": "./dist/x501.cjs"
			}
		},
		"./x509": {
			"import": {
				"types": "./dist/x509.d.ts",
				"default": "./dist/x509.mjs"
			},
			"require": {
				"types": "./dist/x509.d.cts",
				"default": "./dist/x509.cjs"
			}
		}
	},
	"devDependencies": {
		"@types/node": "^20.11.17",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"esbuild": "^0.20.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"prettier": "^3.2.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Exact-Realty/ts-cms-classes.git"
	},
	"files": [
		"dist/**/*"
	],
	"scripts": {
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
		"build": "tsc --emitDeclarationOnly --declarationMap --declaration && node esbuild.mjs",
		"test": "node --loader ts-node/esm test/index.test.ts",
		"prepack": "npm run build",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run lint && git add -A src",
		"postversion": "git push && git push --tags"
	},
	"author": "Exact Realty Limited",
	"license": "ISC",
	"keywords": [
		"asn1",
		"asn.1",
		"der",
		"cms",
		"pkcs10",
		"pkcs#10",
		"x501",
		"x.501",
		"x509",
		"x.509"
	],
	"dependencies": {
		"@exact-realty/asn1-der": "^1.0.1",
		"@exact-realty/crypto-oids": "^20240221.0.0"
	}
}
